<table>
  <tr>
    <th>Username</th>
    <th>Public Key</th>
    <th>Update</th>
  </tr>
 
<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.pub_key %></td>
    <td>
    
      <% form_for user, :url => user_update_path(), :method => :update %>

    </td>
  </tr>
<% end %>
</table>